    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webpack-env": "^1.16.0",
    "babel-loader": "^8.2.2",
    "html-webpack-plugin": "^4.5.1",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
